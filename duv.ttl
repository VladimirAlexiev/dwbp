@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix vann:    <http://purl.org/vocab/vann/> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix voaf:    <http://purl.org/vocommons/voaf#> .
@prefix oa:      <http://www.w3.org/ns/oa#> .
@prefix dcat:    <http://www.w3.org/ns/dcat#> .
@prefix prov:    <http://www.w3.org/ns/prov#> .
@prefix cito:    <http://purl.org/spar/cito#> .
@prefix :        <http://www.w3.org/ns/duv#> .

######################################
### Metadata section              ####
######################################

<http://www.w3.org/ns/duv> a voaf:Vocabulary;
    dcterms:title "Dataset Usage Vocabulary";
    dcterms:description "The Dataset Usage Vocabulary (DUV) is used to describe consumer experiences, citations, and feedback about datasets from the human perspective."@en;
    vann:preferredNamespaceUri "http://www.w3.org/ns/duv";
    vann:preferredNamespacePrefix "duv";
    rdfs:isDefinedBy <http://www.w3.org/TR/vocab-dqv/> ;
    dcterms:created "2015-12-17"^^xsd:date;
    dcterms:publisher <http://www.w3.org/data#W3C>;
    dcterms:type <http://purl.org/adms/assettype/Ontology>;
    dcterms:creator [foaf:name "Bernadette Farias Lóscio"], [foaf:name "Eric G. Stephan"], [foaf:name "Sumit Purohit"] .


#### Classes here ####
######################

:Publication a owl:Class, rdfs:Class ;
  rdfs:label "Publication"@en ;
  rdfs:comment "A citing entity such as a report, paper, or document that cites a dataset."@en .

:Usage a owl:Class, rdfs:Class ;
  rdfs:label "Usage"@en ;
  rdfs:comment "A helpful description of actions that can be performed on a given dataset or distribution."@en .

:DataCitation a owl:Class, rdfs:Class ;
  rdfs:label “DataCitation"@en ;
  rdfs:comment "A reference to a dataset or distribution."@en ;
  rdfs:isDefinedBy http://www.w3.org/ns/duv ;
  rdfs:subClassOf cito:CitationsAct .

:UsageActor a owl:Class, rdfs:Class ;
  rdfs:label "UsageActor"@en ;
  rdfs:comment "An Identifiable entity that perform some usage of the dataset. It can be an application, visualization, or a person."@en ;
  rdfs:subClassOf oa:Annotation .
 
:UsageTool a owl:Class, rdfs:Class ;
  rdfs:label "UsageTool"@en ;
  rdfs:comment "A synopsis describing the way a tool can use a dataset or distribution."@en .

:UserFeedback a owl:Class, rdfs:Class ;
  rdfs:label "User Feedback"@en ;
  rdfs:subClassOf rev:Feedback ;
  rdfs:comment "User feedback on the dataset. Expresses whether the dataset was useful or not, for example."@en .

:RatingFeedback a owl:Class, rdfs:Class ;
  rdfs:label "Rating Feedback"@en ;
  rdfs:comment "Predefined criteria used to express a user opinion about a dataset or distribution using a discrete range of values."@en ;
  rdfs:subclassOf duv:UserFeedback .



##### Properties here #####
###########################

:citationCreator a rdf:Property ;
    rdfs:label "citation creator"@en ;
    rdfs:comment “Creator of the citation itself.”@en ;
    rdfs:range prov:Agent ;
    rdfs:domain duv:DataCitation .

:classification a rdf:Property ;
    rdfs:label "has specific agent category"@en ;
    rdfs:comment " Extension vocabularies may wish to specialize this property to have a range corresponding to a specific  ConceptScheme" @en ;
    rdfs:domain prov:Agent ;
    rdfs:range skos:Concept .

dct:creator a rdf:Property ;
    rdfs:label "creator"@en ;
    rdfs:comment “Author of the cited dataset of distribution”@en ;
    rdfs:range prov:Agent ;
    rdfs:domain duv:DataCitation .


dct:created a rdf:Property ;
    rdfs:label "creator"@en ;
    rdfs:comment “The date associated with the DataCitation.”@en ;
    rdfs:range rdfs:Literal ;
    rdfs:domain duv:DataCitation .

cito:hasCitingEntity a rdf:Property ;
    rdfs:label "has citing entity"@en ;
    rdfs:comment "Describes the publication that cites the dataset."@en ;
    rdfs:range duv:Publication ;
    rdfs:domain duv:Citation .

:hasAuthor a rdf:Property ;
    rdfs:label "has author"@en ;
    rdfs:comment "Describes the agent who provides the citation."@en ;
    rdfs:range prov:Agent ;
    rdfs:domain duv:Citation .

:hasPublisher a rdf:Property ;
    rdfs:label "has author"@en ;
    rdfs:comment "Describes the publisher who publishes the citation."@en ;
    rdfs:range prov:Agent ;
    rdfs:domain duv:Citation .

:hasUsage a rdf:Property ;
    rdfs:label "has usage"@en ;
    rdfs:comment "Description of the fact that the dataset has a Usage experience associated with it" @en ;
    rdfs:range duv:Usage ;
    rdfs:domain dcat:Dataset ;
    rdfs:domain dcat:Distribution .

:hasCitedEntity a rdf:Property ;
    rdfs:label "has cited entity"@en ;
    rdfs:comment "Description of the fact that the citation belongs to a dcat dataset" @en ;
    rdfs:range dcat:Dataset ;
    rdfs:range dcat:Distribution ;
    rdfs:domain duv:Citation .

:performs a rdf:Property ;
    rdfs:label "performs"@en ;
    rdfs:comment "Describes the Usage an actor provides" @en ;
    rdfs:domain duv:UsageActor ;
    rdfs:range duv:Usage .

:hasUsageTool a rdf:Property ;
    rdfs:label "has usage tool"@en ;
    rdfs:comment "Describes the tool that provides the Usage " @en ;
    rdfs:range duv:UsageTool ;
    rdfs:domain duv:Usage .


:performedBy a rdf:Property ;
    rdfs:label "performed by"@en ;
    rdfs:comment "Describes the actor who provides the Usage " @en ;
    rdfs:range duv:UsageActor ;
    rdfs:domain duv:Usage .

oa:hasTarget a rdf:Property ;
    rdfs:label "has target"@en ;
    rdfs:comment "Describes the dataset for which the User Feedback is provided" @en ;
    rdfs:range dcat:Dataset ;
    rdfs:range dcat:Distribution ;
    rdfs:domain duv:UserFeedback .

:hasUserFeedback a rdf:Property ;
    rdfs:label "has user feedback"@en ;
    rdfs:comment "Describes the user feedback provided for a dcat dataset" @en ;
    rdfs:domain dcat:Dataset ;
    rdfs:domain dcat:Distribution ;
    rdfs:range duv:UserFeedback .


:hasRatingFeedback a rdf:Property ;
    rdfs:label "has rating"@en ;
    rdfs:comment "Describes the user rating feedback provided for a dcat dataset" @en ;
    rdfs:domain duv:RatingFeedback ;
    rdfs:range skos:Concept .

:hasRole a rdf:Property ;
    rdfs:label "has role"@en ;
    rdfs:comment "Describes the role usage actor has in providing duv:Usage" @en ;
    rdfs:domain duv:UsageActor ;
    rdfs:range skos:Concept .

:hasSpecificAgentCategory a rdf:Property ;
    rdfs:label "has specific agent category"@en ;
    rdfs:comment "Describes the type of prov:Agent creating a duv:Citation" @en ;
    rdfs:domain prov:Agent ;
    rdfs:range skos:Concept .

:hasUsageType a rdf:Property ;
    rdfs:label "has usage type"@en ;
    rdfs:comment "Describes the type of the usage performed by actor" @en ;
    rdfs:domain duv:Usage ;
    rdfs:range skos:Concept .
